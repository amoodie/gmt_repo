#!/bin/bash

# Andrew J Moodie
# Feb 2015
# An example for teaching scripting with GMT
gmt pscoast -R-87/-69/31/45 -JM10c -Xc -Yc -B2 -V -Dh -A0/0/2 -Glightgrey -N2/0.2p -W0.1p -K > map.ps

gmt pscoast -R-87/-69/31/45 -JM10c -Xc -Yc -V -Dh -Ir/0.3p,blue -K -O >> map.ps

gmt pscoast -R-87/-69/31/45 -JM10c -Xc -Yc -V -Dh -I1/0.8p,blue -O >> map.ps
